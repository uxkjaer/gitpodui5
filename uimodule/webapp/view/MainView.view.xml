 <mvc:View controllerName="au.com.agilux.gitpodUi5.controller.MainView"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
      <Page title="{i18n>title}">
        <content>
         <List id="PeopleList"
            headerText="{i18n>otherPage.listHeader}"
            items="{/Orders}" itemPress="onListItemPress">
            <StandardListItem title="{OrderID}" description="{CustomerID}" type="Navigation"  />
			</List>
        </content>
      </Page>

</mvc:View>
